<form action="/vehicles/<%=vehicle._id%>?_method=PUT" method="POST">
    <div class="form-group">
        <label for="make">MAKE:</label>
        <input id="make" name="vehicle[make]" class="form-control" value=<%=vehicle.make%> >
    </div>
    <div class="form-group">
        <label for="model">MODEL:</label>
        <input id="model" name="vehicle[model]" class="form-control" value=<%=vehicle.model%>>
    </div>
    <div class="form-group">
        <label for="year">YEAR:</label>
        <input id="year" name="vehicle[year]" class="form-control" value=<%=vehicle.year%>>
    </div>
    <div class="form-group">
        <label for="color">COLOR:</label>
        <input id="color" name="vehicle[color]" class="form-control" value=<%=vehicle.color%>>
    </div>
    <button type="submit" class="btn btn-primary">Update</button>
</form>